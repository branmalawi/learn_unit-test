{
  "name": "jest",
  "version": "1.0.0",
  "description": "belajar cara menggunakaj jest (node js unit test)",
  "main": "./src/index.js",
  "scripts": {
    "test": "jest"
  },
  "author": "branmalawi",
  "license": "ISC",
  "type": "module",
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./src/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.5",
    "jest": "^29.5.0"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.22.5"
  }
}
